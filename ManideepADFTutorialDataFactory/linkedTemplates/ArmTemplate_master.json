{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ManideepADFTutorialDataFactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureHDInsightLinkedService_clusterPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_clusterSshPassword": {
			"type": "secureString",
			"metadata": "Secure string for 'clusterSshPassword' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'AzureHDInsightLinkedService'"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
			"type": "string",
			"defaultValue": "ae0e513c-af7b-46c5-ad8d-f5f540fa3d46"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
			"type": "string",
			"defaultValue": "Projects"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "76fd06de-0994-402c-bd3e-ea026c970572"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "1d915f78-8891-4e8f-a49c-e2caa544b388"
		},
		"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
			"type": "string",
			"defaultValue": "Mani_Prj_Cluster"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ManideepADFTutorialDataFactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureHDInsightLinkedService_clusterPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterPassword')]"
					},
					"AzureHDInsightLinkedService_clusterSshPassword": {
						"value": "[parameters('AzureHDInsightLinkedService_clusterSshPassword')]"
					},
					"AzureHDInsightLinkedService_servicePrincipalKey": {
						"value": "[parameters('AzureHDInsightLinkedService_servicePrincipalKey')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_hostSubscriptionId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterResourceGroup')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_tenant": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_tenant')]"
					},
					"AzureHDInsightLinkedService_properties_typeProperties_clusterUserName": {
						"value": "[parameters('AzureHDInsightLinkedService_properties_typeProperties_clusterUserName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}